Select * from HR.EMPLOYEES;
SELECT FIRST_NAME "NOME",
LAST_NAME "SOBRENOME",
EMAIL , 
SALARY "SALARIO",
SALARY * 12 "SALARIO ANUAL",
HIRE_DATE "DATA CONTRATAÇÃO",
* FROM HR.EMPLOYEES;

